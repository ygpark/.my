source $HOME/.my/config/git/_gitrc

alias vi-bashrc="vi /home/ygpark/.my/config/_mybashrc"
alias vi-vimrc="vi /home/ygpark/.my/config/vim/_vimrc"
alias vi-gitconfig="vi /home/ygpark/.my/config/git/_gitconfig"
alias vi-gitman="vi /home/ygpark/.my/config/git/_gitman"
alias vi-githelpme="vi /home/ygpark/.my/config/git/_githelpme"

alias my="cd $HOME/.my/"
alias myconfig="cd $HOME/.my/config"
alias myscript="cd $HOME/.my/script"
alias myvim="cd $HOME/.my/config/vim"
alias myman="cd $HOME/.my/man"
alias myhello="cd $HOME/.my/hello"

alias init-cmake="cp $HOME/.my/hello/make/cmake/99-template/CMakeLists.txt ."
alias init-scons="cp $HOME/.my/hello/make/scons/99-template/SConstruct ."
alias init-qmake="cp $HOME/.my/hello/make/qmake/99-template/project.pro ."

alias init-hello-cpp="cp /home/ygpark/.my/hello/cpp/00-helloworld/* ."

